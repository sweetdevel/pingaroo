{% extends "layouts/app.phtml" %} 
 
{% block headers_title %}Pingaroo{% endblock %}
{% block body_title %}View Country{% endblock %}

{% block content %}
    {{ partial( 
        'common/partials/table',
        [
            'enabled' : false,
            'model' : country,
            'columns' : 
            [
                'id' : 'ID',
                'code' : 'Code',
                'name' : 'Name',
                'createdAt' : 'Created At',
                'updatedAt' : 'Updated At'
            ]
        ]
       ) 
    }}
    
    <a href="{{ url('country') }}">
       <button type="button" class="btn btn-sm btn-default">Back</button>
    </a>
{% endblock %}

{% block bottom %}
{% endblock %}